{
  "name": "plancy-pure-boilerplate",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "plancy-pure-boilerplate"
  ],
  "license": "MIT",
  "author": "plancy",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "engines": {
    "node": ">=4.x"
  },
  "source": "src/index.html",
  "targets": {
    "main": {
      "engines": {
        "node": ">=4.x"
      }
    },
    "module": {
      "engines": {
        "node": ">=8.x"
      }
    },
    "browser": {
      "engines": {
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  },
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  },
  "scripts": {
    "build": "parcel build",
    "dev": "parcel",
    "prestart": "yarn build",
    "start": "parcel serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.0.0-rc.0",
    "@parcel/transformer-image": "2.0.0-rc.0",
    "@parcel/transformer-webmanifest": "2.0.0-rc.0",
    "parcel": "^2.0.0-rc.0"
  }
}
